{
  "name": "feliz.preset-npw",
  "version": "0.1.3",
  "description": "A feliz.js preset using: Hapi, Nunjucks, PostCSS and Webpack",
  "main": "lib/index.js",
  "author": "Héctor Menéndez <etor@gik.mx",
  "license": "MIT",
  "analyze": true,
  "keywords": [
    "feliz",
    "preset",
    "hapi",
    "nunjucks",
    "postcss",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gikmx/feliz.preset-npw"
  },
  "bugs": {
    "url": "https://github.com/gikmx/feliz.preset-npw/issues"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.11.0",
    "cssnano": "^3.7.3",
    "feliz-bundle-js": "0.0.18",
    "feliz-bundle-postcss": "0.0.4",
    "feliz-logger": "0.0.1",
    "feliz-static": "0.0.2",
    "feliz-views": "0.0.12",
    "feliz.error": "0.0.2",
    "feliz.plug": "0.0.2",
    "feliz.preset": "0.0.2",
    "feliz.util": "0.0.5",
    "lost": "^7.0.3",
    "postcss-conditionals": "^2.0.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-for": "^2.0.3",
    "postcss-import": "^8.1.2",
    "postcss-mixins": "^5.0.0",
    "postcss-property-lookup": "^1.2.1",
    "postcss-simple-vars": "^3.0.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "feliz.test": "0.1.0-beta.13",
    "husky": "^0.11.5",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^1.3.7",
    "nodemon": "^1.10.2",
    "tap-spec": "^4.1.1"
  },
  "scripts": {
    "start": "nodemon ./test -x 'npm run docs && npm run debug -s' -w ./lib -w ./test | tap-spec",
    "docs": "jsdoc2md -t ./template.hbs 'lib/**/*.js' > README.md",
    "postdocs": "git add --force README.md",
    "test": "tape ./test | tap-spec",
    "debug": "node-debug --cli --no-debug-brk --debug-port 5902 --web-port 9902 --save-live-edit --no-preload",
    "debug:watch": "nodemon ./test -x 'npm run debug -s' -w ./lib -w ./test | tap-spec",
    "cover": "npm run cover:stats && npm run cover:send",
    "cover:check": "npm run cover:stats && istanbul check-coverage",
    "cover:stats": "istanbul cover tape -- ./test",
    "cover:send": "cat ./coverage/lcov.info | coveralls",
    "postversion": "git add package.json",
    "precommit": "npm test && npm run cover:check && npm run docs && npm version prerelease --no-git-tag-version"
  }
}
